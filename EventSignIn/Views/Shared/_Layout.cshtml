<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    <link href="/Content/plugins/bootstrap-datetimepicker.min.css" rel="stylesheet"/>
</head>
<body>
    @RenderBody()

    @Scripts.Render("~/bundles/jquery")
    <script src="/Scripts/plugins/bootstrap-datetimepicker.min.js"></script>
    @Scripts.Render("~/bundles/site")
    @RenderSection("scripts", required: false)
    <div id="page_modal" class="modal hide fade" tabindex="-1" role="dialog">
        
    </div>
</body>
</html>
